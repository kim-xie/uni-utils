{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false, // 🈲此模块转成 CommonJS
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime",{
      "corejs": {"version": 3},
      "useESModules": true
    }]
  ]
}

